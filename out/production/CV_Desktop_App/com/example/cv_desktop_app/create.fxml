<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cv_desktop_app.CreateController">
    <children>
        <Label layoutX="38.0" layoutY="210.0" text="Attribute Name:" />
        <TextField fx:id="attrName" layoutX="38.0" layoutY="241.0" />
        <Label layoutX="38.0" layoutY="275.0" text="Value:" />
        <TextField fx:id="valueText" layoutX="38.0" layoutY="302.0" />
        <Button fx:id="addAttrButton" layoutX="97.0" layoutY="341.0" mnemonicParsing="false" onAction="#addAttributeToTable" text="Add Attribute" />
        <TableView fx:id="attrTable" layoutX="34.0" layoutY="131.0" onMouseClicked="#selectCV" prefHeight="57.0" prefWidth="531.0" />
        <Label layoutX="97.0" layoutY="87.0" prefHeight="18.0" prefWidth="106.0" text="Name of CV Owner:" />
        <TextField fx:id="CVName" layoutX="211.0" layoutY="84.0" />
        <TextField fx:id="tagText" layoutX="242.0" layoutY="280.0" />
        <Label layoutX="242.0" layoutY="258.0" text="Tag:" />
        <Button fx:id="addTagButton" layoutX="331.0" layoutY="315.0" mnemonicParsing="false" text="Add Tag" />
        <Button fx:id="createCVButton" layoutX="487.0" layoutY="328.0" mnemonicParsing="false" onAction="#createCV" text="Create CV" />
        <Button layoutX="369.0" layoutY="84.0" mnemonicParsing="false" onAction="#addNameToTable" text="Add to Table" />
    </children>
</AnchorPane>
